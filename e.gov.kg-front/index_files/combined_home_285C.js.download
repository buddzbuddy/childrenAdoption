        } else {
            heroOptions['x'] = endPositions.hero['x'];
            textOptions['x'] = endPositions.textItems['x'];
        }

        foreground
            .to(ui.hero, .6, heroOptions)
            .to(ui.textItems, 1, textOptions, 0)
            .set(ui.heroWrap, { display: 'none' })
            .set(ui.textWrap, { display: 'none' })
            .set(ui.textItems, { display: 'none' });

        return foreground;
    }

    function foregroundIn(frameNum, direction) {
        var $frame = $('.slider-foreground__frame[data-frame-id=' + frameNum + ']'),
            ui = {
                heroWrap: $frame.find('.slider-foreground__frame__hero-wrap'),
                hero: (isFallbackAnimation()) ? $frame.find('.slider-foreground__frame__hero-wrap > div') : $frame.find('.slider-foreground__frame__hero'),
                textWrap: $frame.find('.slider-foreground__frame__text-wrap'),
                textItems: $frame.find('.slider-foreground__frame__text > *')
            },
            foreground = new TimelineMax(),
            heroOptions = {
                opacity: 1,
                ease: Power1.easeOut
            },
            textOptions = {
                opacity: 1,
                zIndex: 1,
                ease: Power1.easeOut
            },
            beginPositions = {};

        if (isFallbackAnimation()) {
            heroOptions['left'] = 0;
            textOptions['left'] = 0;

            beginPositions = {
                hero: {
                    left: getElementsAnimationPositions(direction).hero['x']
                },
                textItems: {
                    left: getElementsAnimationPositions(direction).textItems['x'],
                    display: 'block'
                }
            }
        } else {
            heroOptions['x'] = 0;
            textOptions['x'] = 0;

            beginPositions = {
                hero: {
                    x: getElementsAnimationPositions(direction).hero['x']
                },
                textItems: {
                    x: getElementsAnimationPositions(direction).textItems['x'],
                    display: 'block'
                }
            }
        }

        foreground
            .set(ui.hero, beginPositions.hero)
            .set(ui.textItems, beginPositions.textItems)
            .set(ui.heroWrap, { display: 'block' })
            .set(ui.textWrap, { display: 'table' })
            .to(ui.hero, .7, heroOptions)
            .staggerTo(ui.textItems, .4, textOptions, .03, .6)
            .call(function () {
                if ($frame.hasClass(SKIP_ANIMATION_CLASS)) {
                    $frame.removeClass(SKIP_ANIMATION_CLASS);
                }
            });

        return foreground;
    }

    function goToFrame(prevFrame, nextFrame, framesAmount, bgColors) {
        //nextFrame - zerobased order
        var mainTl = new TimelineMax({
            onComplete: function () {
                inProgressTl = -1;
            }
        }),
            direction = getSlideDirection(prevFrame, nextFrame, framesAmount);

        if (prevFrame === nextFrame) return; //чтобы лишний раз не запускать анимацию

        updateNav(nextFrame);

        if (inProgressTl !== -1) {
            inProgressTl.progress(1); //закончить анимацию, если идет перед началом следующей
        }

        inProgressTl = mainTl;

        mainTl
            .add(backlight(bgColors[nextFrame]), 0)
            .add(cycleSlideFilmTo(nextFrame, prevFrame, bgColors.length), 0)
            .add(foregroundOut(prevFrame, direction), 0)
            .add(foregroundIn(nextFrame, direction), .3);
    }

    function updateNav(n) {
        var $navItems = $('.carousel__nav__item');

        $navItems.removeClass('active');
        $navItems.filter('[data-frame-to=' + n + ']').addClass('active');
    }


    this.initialize = function () {
        var ui = {
            nextButton: $('.carousel__control_next'),
            prevButton: $('.carousel__control_prev'),
            dots: $('.carousel__nav__item'),
            sliderWrap: $('.carousel_home_wrap'),
            frames: $('.slider-film__frame[data-frame-id]')
        },
            framesAmount = ui.frames.length,
            bgColors = extractOptions(ui.frames),
            //t, //timer for looping
            currentFrameId = 0,
            animated;

        function next() {
            var p = currentFrameId;

            currentFrameId = (currentFrameId === framesAmount - 1) ? 0 : currentFrameId + 1;
            goToFrame(p, currentFrameId, framesAmount, bgColors);
        }

        function prev() {
            var p = currentFrameId;

            currentFrameId = (currentFrameId === 0) ? framesAmount - 1 : currentFrameId - 1;
            goToFrame(p, currentFrameId, framesAmount, bgColors);
        }

        /*function loop() {
            t = setInterval(function () {
                next();
            }, (ANIMATION_DURATION + ANIMATION_DELAY) * 1000);
        }*/

        function pause() {
            //clearInterval(t);
        }

        //--> bind to DOM
        ui.prevButton.on('click', function () {
            pause();
            prev();
        });
        ui.nextButton.on('click', function () {
            pause();
            next();
        });

        ui.sliderWrap.swipe({
            swipeStatus: function (event, phase, direction, distance) {
                if (phase == 'move' && distance >= 5 && !animated) {
                    if (direction === 'left') {
                        pause();
                        next();
                        animated = true;
                    }
                    if (direction === 'right') {
                        pause();
                        prev();
                        animated = true;
                    }
                }
                if (phase != 'move') {
                    animated = false;
                }
            },
            threshold: 200,
            maxTimeThreshold: 5000,
            fingers: 'all',
            allowPageScroll: "auto"
        });

        ui.dots.on('click', function () {
            var p = currentFrameId;

            currentFrameId = parseInt($(this).attr('data-frame-to'));
            pause();
            goToFrame(p, currentFrameId, framesAmount, bgColors);
        });

        /*ui.sliderWrap
            .on('mouseenter', pause)
            .on('mouseleave', loop);*/

        $(window)
            .on('blur', function () {
                pause();
            });
        //<-- bind to DOM

        goToFrame(null, 0, framesAmount, bgColors);
        //loop();
    }

}, [jQuery]);
