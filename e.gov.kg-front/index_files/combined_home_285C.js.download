        var slide,
            slideOptions = {},
            position;

        if (frameNum === framesAmount - 1 && prevFrameNum === 0) {
            position = (framesAmount - 1 + 2) * -1 * SLIDE_WIDTH + SLIDE_MARGIN_LEFT;

            if (isFallbackAnimation()) {
                slideOptions['left'] = position;
            } else {
                slideOptions['x'] = position;
            }

