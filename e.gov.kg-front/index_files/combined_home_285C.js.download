            if (isFallbackAnimation()) {
                slideOptions['left'] = position;
            } else {
                slideOptions['x'] = position;
            }

            slide = new TimelineMax();

            slide
                .add(slideFilmTo(framesAmount + 2))
                .set($('.slider-film'), slideOptions);
        } else {
            slide = slideFilmTo(frameNum + 2);
