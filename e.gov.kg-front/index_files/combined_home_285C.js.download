        }

        return options;
    }

    function backlight(color) {
        //смена фонового цвета
        var light = new TimelineMax();

        light.to($('.carousel_home_wrap'), ANIMATION_DURATION, {
            backgroundColor: color,
            ease: Power1.easeOut
        });

        return light;
    }

    function slideFilmTo(frameNum) {
        //frameNum - 1-based order
        var slide = new TimelineMax(),
            slideOptions = {
                ease: Power1.easeOut
            },
            position = (frameNum) * -1 * SLIDE_WIDTH + SLIDE_MARGIN_LEFT;

