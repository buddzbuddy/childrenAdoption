    return {
        init: function() {
            function toggleFact() {
                var current = factsList.filter('.main-fact_first');
                if (current.next().size() > 0) {
                    current.removeClass('main-fact_first').next().addClass('main-fact_first');
                } else {
                    current.removeClass('main-fact_first');
                    factsList.first().addClass('main-fact_first');
                }
            }

            var factsList = $('.main-fact'),
                size;

            factsList.removeClass("main-fact_first");
            size = factsList.size();

            factsList.eq([Math.floor(Math.random() * size)]).addClass("main-fact_first");

            $('#logoCarousel').jcarousel({
                scroll: 1,
                itemVisibleOutCallback: {
                    onBeforeAnimation: toggleFact
                },
                wrap: 'circular'
            });
        }
    };
}(jQuery));

