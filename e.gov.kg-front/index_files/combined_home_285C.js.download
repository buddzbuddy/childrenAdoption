        } else {
            slideOptions['x'] = position;
        }

        slide.to($('.slider-film'), ANIMATION_DURATION, slideOptions);

        return slide;
    }

    function cycleSlideFilmTo(frameNum, prevFrameNum, framesAmount) {
        //первые два и последние два слайда используются только для циклического перехода из конца в начало и наоборот
        //порядок такой: BLANK1 BLANK2 0 1 2 3 ... 5 BLANK3 BLANK4
        //например: при переходе от слайда 0 к последнему сначала будет анимированная прокрутка 
